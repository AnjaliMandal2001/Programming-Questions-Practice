import java.util.*;

class Largest
 {
  public static void main(String[] args)
 {
    int[] marks = {23, 45, 59, 10, 34, 67, 19,100};
    int answer=get_LargestNumber(marks);
    System.out.println(answer);
    
  }

  public static int get_LargestNumber(int[] marks) 
{
    int largest=Integer.MIN_VALUE;
    for(int i =0;i<marks.length;i++)
    {
     if(largest<marks[i])
     {
       largest=marks[i]; 
     }

    }
  return largest;
 }
}
